{
  "DiscordWebhook": {
    "Url": "discord-webhook-url-here"
  },
  "Auth": {
    "Self": "https://identity.domain.here",
    "AccessToken": {
      "JwtIssuer": "issuer-here",
      "JwtAudience": "audience-here",
      "JwtSecrets": [
        "access-secret-here (first one will be used to create new tokens)"
      ]
    },
    "RefreshToken": {
      "JwtIssuer": "issuer-here",
      "JwtAudience": "audience-here",
      "JwtSecrets": [
        "refresh-secret-here (first one will be used to create new tokens)"
      ]
    },
    "Test": {
      "Scopes": ["scopes-here"],
      "ClientId": "test-provider-client-id-here",
      "ClientSecret": "test-provider-secret-here"
    },
    "Discord": {
      "Scopes": ["scopes-here"],
      "ClientId": "discord-provider-client-id-here",
      "ClientSecret": "discord-provider-secret-here"
    },
    "Github": {
      "Scopes": ["scopes-here"],
      "ClientId": "github-provider-client-id-here",
      "ClientSecret": "github-provider-secret-here"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "production-connection-string-here"
  }
}